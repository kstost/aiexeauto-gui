{
  "name": "aiexeauto",
  "version": "1.0.158",
  "main": "aiexeauto.js",
  "type": "module",
  "bin": {
    "aiexeauto": "aiexeauto.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cm": "codemerger . -c -i .mergeignore",
    "electron": "electron .",
    "tm": "electron timeMachine.js",
    "build": "cross-env NODE_ENV=production electron-builder"
  },
  "keywords": [],
  "author": {
    "name": "Kim Seungtae",
    "email": "monogatree@gmail.com",
    "url": "https://www.youtube.com/@%EC%BD%94%EB%93%9C%EA%B9%8E%EB%8A%94%EB%85%B8%EC%9D%B8"
  },
  "license": "MIT",
  "description": "Safe AI-Agent AIEXE Auto",
  "dependencies": {
    "@langchain/core": "0.3.42",
    "@langchain/openai": "^0.4.2",
    "axios": "^1.7.8",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "express": "^4.21.1",
    "langchain": "0.3.19",
    "open": "^10.1.0",
    "openai": "^4.83.0",
    "ora": "^8.1.1",
    "puppeteer": "^23.8.0",
    "tar": "^7.4.3",
    "turndown": "^7.2.0",
    "@modelcontextprotocol/sdk": "^1.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@electron/notarize": "^2.3.2",
    "@electron/osx-sign": "^1.3.0",
    "@eslint/js": "^9.20.0",
    "cross-env": "^7.0.3",
    "electron": "^30.0.7",
    "electron-builder": "^24.13.3",
    "eslint": "^9.20.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.14.0"
  },
  "build": {
    "appId": "com.cokac.aiexeauto",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "./assets/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "identity": "Seungtae Kim"
    },
    "win": {
      "target": "nsis",
      "icon": "./assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./assets/icon.ico",
      "uninstallerIcon": "./assets/icon.ico",
      "installerHeaderIcon": "./assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "./assets/icon.png"
    },
    "asarUnpack": [
      "node_modules/@langchain/core",
      "node_modules/@langchain/openai",
      "node_modules/langchain"
    ]
  }
}